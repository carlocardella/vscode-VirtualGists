// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "out": false, // set this to true to hide the "out" folder with the compiled JS files
        "dist": false // set this to true to hide the "dist" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true, // set this to false to include "out" folder in search results
        "dist": true // set this to false to include "dist" folder in search results
    },
    // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
    "typescript.tsc.autoDetect": "off",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#9cb5a1",
        "activityBar.background": "#9cb5a1",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#6e58c7",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#9cb5a1",
        "statusBar.background": "#7f9f85",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#65866b",
        "statusBarItem.remoteBackground": "#7f9f85",
        "statusBarItem.remoteForeground": "#15202b",
        "tab.activeBorder": "#9cb5a1",
        "titleBar.activeBackground": "#7f9f85",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#7f9f8599",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#7f9f85"
}